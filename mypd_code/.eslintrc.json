{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "no-unused-vars": "off",
	"no-mixed-spaces-and-tabs":"off",
	"vue/max-attributes-per-line":"off",
	"no-debugger":"off",
	"no-unreachable":"off",
	"vue/multiline-html-element-content-newline":"off",
	"vue/html-indent":"off"
  } ,"globals": {
        "_": "writable","evt":"writable"
    }
}